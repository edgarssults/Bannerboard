<configuration>
    <system.webServer>
      <rewrite>
        <rules>
          <rule name="Rewrite to bannerboard.net">
            <match url="^(.*)$" />
            <action type="Redirect" url="https://www.bannerboard.net" />
          </rule>
        </rules>
      </rewrite>
    </system.webServer>
</configuration>